{
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ],
  "presets": [
    ["env", {
      "modules": false
    }],
    "react",
    "stage-0"
  ],
  "env": {
    "es6": {
      "plugins": [
        "transform-runtime",
        ["module-resolver", {
            "root": ["./src"]
        }]
      ],
      "presets": [
        ["env", {
          "loose": true,
          "modules": false
        }],
        "stage-0",
        "react"
      ],
    }
  }
}
